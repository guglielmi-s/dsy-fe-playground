
.navbar 
    bg-color($dsy-bg-navbar)
    height pxToRem(59.38)
    display flex
    flex-direction row
    justify-content space-evenly
    align-items center

    &__list 
        display flex
        flex-direction row
        justify-content flex-start
        align-items center


    &__list a 
        font-family $dsy-family
        font-size pxToRem(14)
        color $dsy-primary_text
        text-decoration none
        margin-right pxToRem(41)

    &__btnBox 
        display flex
        flex-direction row
        align-items center
        justify-content center
    

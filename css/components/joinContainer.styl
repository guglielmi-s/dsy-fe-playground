.joinContainer
    display grid
    grid-template-columns repeat(12, 1fr)
    padding 50px
    background-image linear-gradient $dsy-gradient-color  $dsy-primary_text


    &__footer
        margin-top 50px
        grid-column-start 6 
        grid-column-end 7 
   


    &__footer p 
        font-size $dsy-size - 0.3rem
        color $dsy-secondary_text-lg
